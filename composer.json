{
  "name": "chijioke-ibekwe/raven",
  "description": "Multi-channel Laravel notification sender",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "sendgrid",
    "notifications",
    "sendgrid-api",
    "laravel-package"
  ],
  "authors": [
    {
      "name": "Chijioke Ibekwe",
      "email": "ibekwe.chijioke18@gmail.com"
    }
  ],
  "homepage": "https://github.com/chijioke-ibekwe/raven",
  "require": {
    "sendgrid/sendgrid": "~7",
    "aws/aws-sdk-php": "^3.300",
    "phpmailer/phpmailer": "^6.9",
    "vonage/client": "^4.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "ChijiokeIbekwe\\Raven\\": "src/",
      "ChijiokeIbekwe\\Raven\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ChijiokeIbekwe\\Raven\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ChijiokeIbekwe\\Raven\\RavenServiceProvider"
      ]
    }
  }
}
